@using DoktorKapisi.Controllers
@{
    Layout = null;
}

<!DOCTYPE html>
<html class="side-header">
<head>

    <!-- Basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <title>Porto - Multipurpose Website Template</title>

    <meta name="keywords" content="WebSite Template" />
    <meta name="description" content="Porto - Multipurpose Website Template">
    <meta name="author" content="okler.net">

    <!-- Favicon -->
    <link rel="shortcut icon" href="img/favicon.ico" type="image/x-icon" />
    <link rel="apple-touch-icon" href="~/DoktorKapısı/img/apple-touch-icon.png">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1.0, shrink-to-fit=no">

    <!-- Web Fonts  -->
    <link id="googleFonts" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800%7CShadows+Into+Light&display=swap" rel="stylesheet" type="text/css">

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/animate/animate.compat.css">
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/simple-line-icons/css/simple-line-icons.min.css">
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/owl.carousel/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/owl.carousel/assets/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/magnific-popup/magnific-popup.min.css">

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/DoktorKapısı/css/theme.css">
    <link rel="stylesheet" href="~/DoktorKapısı/css/theme-elements.css">
    <link rel="stylesheet" href="~/DoktorKapısı/css/theme-blog.css">
    <link rel="stylesheet" href="~/DoktorKapısı/css/theme-shop.css">

    <!-- Current Page CSS -->
    <link rel="stylesheet" href="~/DoktorKapısı/vendor/circle-flip-slideshow/css/component.css">

    <!-- Skin CSS -->
    <link id="skinCSS" rel="stylesheet" href="~/DoktorKapısı/css/skins/default.css">

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/DoktorKapısı/css/custom.css">

    <!-- Head Libs -->
    <script src="~/DoktorKapısı/vendor/modernizr/modernizr.min.js"></script>

</head>
<body data-plugin-page-transition>
    <div class="body">
        <header id="header" class="side-header d-flex">
            <div class="header-body">
                <div class="header-container container d-flex h-100">
                    <div class="header-column flex-row flex-lg-column justify-content-center h-100">
                        <div class="header-row flex-row justify-content-start justify-content-lg-center py-lg-5">
                            <div class="header-logo">
                                <a href="/Doktor/Index/">
                                    <img alt="DoktorKapisi" width="70" height="70"
                                         src="~/DoktorKapısı/img/demos/DoktorKapisi/DoktorKapısıLOGO.png">
                                </a>
                            </div>
                        </div>
                        <div class="header-row header-row-side-header flex-row h-100 pb-lg-5">
                            <div class="header-nav header-nav-links header-nav-links-side-header header-nav-links-vertical header-nav-links-vertical-dropdown align-self-start">
                                <div class="header-nav-main header-nav-main-square header-nav-main-dropdown-no-borders header-nav-main-effect-4 header-nav-main-sub-effect-1">
                                    <nav class="collapse">
                                        <ul class="nav nav-pills" id="mainNav">
                                            <li class="dropdown">
                                                <a class="dropdown-item dropdown-toggle active" href="/Doktor/Index/">
                                                    PROFİL
                                                </a>
                                            </li>
                                            <li class="dropdown dropdown-mega">
                                                <a class="dropdown-item dropdown-toggle" href="#">
                                                    RANDEVULAR
                                                </a>
                                            </li>
                                            <li class="dropdown">
                                                <a class="dropdown-item dropdown-toggle" href="#">
                                                    GELEN MESAJLAR
                                                </a>
                                            </li>
                                            <li class="dropdown">
                                                <a class="dropdown-item dropdown-toggle" href="#">
                                                    İSTATİSTİKLER
                                                </a>
                                            </li>
                                            <li class="dropdown">
                                                <a class="dropdown-item dropdown-toggle" href="/Default/Index/">
                                                    ÇIKIŞ
                                                </a>
                                            </li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <div role="main" class="main">
            <br /> <br /> <br />
            <div id="examples" class="container py-2">
                <div class="row">
                    <div class="col-lg-9 order-1 order-lg-2">
                        <div class="overflow-hidden mb-1">
                            <h2 class="font-weight-normal text-7 mb-0"><strong class="font-weight-extra-bold">Profil</strong> düzenlemeleri</h2>
                        </div>
                        <div class="overflow-hidden mb-4 pb-3">
                        </div>
                        <div class="card mb-4">
                            <div class="card-body">
                                <form role="form" class="needs-validation" novalidate="novalidate">
                                    <!-- Ad -->
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label line-height-9 pt-2 text-2 required">Ad</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" placeholder="Ad" aria-label="Ad" value="@(GirisYapanDoktor.AktifDoktor?.Ad ?? "")" disabled="">
                                        </div>
                                    </div>
                                    <!-- Soyad -->
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label line-height-9 pt-2 text-2 required">Soyad</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" placeholder="Ad" aria-label="Ad" value="@(GirisYapanDoktor.AktifDoktor?.Soyad ?? "")" disabled="">
                                        </div>
                                    </div>
                                    <!-- Mail -->
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label line-height-9 pt-2 text-2 required">Mail</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" placeholder="Mail" aria-label="Mail" value="@(GirisYapanDoktor.AktifDoktor?.Mail ?? "")" disabled="">
                                        </div>
                                    </div>
                                    <!-- Şifre -->
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label line-height-9 pt-2 text-2 required">Şifre</label>
                                        <div class="col-lg-9">
                                            <input class="form-control text-3 h-auto py-2" type="password" value="@(GirisYapanDoktor.AktifDoktor?.Sifre ?? "")" disabled="">
                                        </div>
                                    </div>
                                    <!-- Uzmanlık Alanı -->
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label line-height-9 pt-2 text-2 required">Uzmanlık Alanı</label>
                                        <div class="col-lg-9">
                                            <input class="form-control text-3 h-auto py-2" type="text" value="@(GirisYapanDoktor.AktifDoktor?.UzmanlikAlani ?? "")" disabled="">
                                        </div>
                                    </div>
                                    <!-- Telefon -->
                                    <div class="form-group  row">
                                        <label class="col-lg-3 col-form-label form-control-label line-height-9 pt-2 text-2 required">Telefon</label>
                                        <div class="col-lg-9">
                                            <input type="number" class="form-control h-auto py-2" value="@(GirisYapanDoktor.AktifDoktor?.Telefon ?? "")" disabled="">
                                        </div>
                                    </div>
                                    <!-- Şehir -->
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label line-height-9 pt-2 text-2 required">Şehir</label>
                                        <div class="col-lg-9">
                                            <input type="text" class="form-control h-auto py-2" value="@(GirisYapanDoktor.AktifDoktor?.Sehir ?? "")" disabled="">
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Vendor -->
    <script src="~/DoktorKapısı/vendor/jquery/jquery.min.js"></script>
    <script src="~/DoktorKapısı/vendor/jquery.appear/jquery.appear.min.js"></script>
    <script src="~/DoktorKapısı/vendor/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/DoktorKapısı/vendor/jquery.cookie/jquery.cookie.min.js"></script>
    <script src="~/DoktorKapısı/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/DoktorKapısı/vendor/jquery.validation/jquery.validate.min.js"></script>
    <script src="~/DoktorKapısı/vendor/jquery.easy-pie-chart/jquery.easypiechart.min.js"></script>
    <script src="~/DoktorKapısı/vendor/jquery.gmap/jquery.gmap.min.js"></script>
    <script src="~/DoktorKapısı/vendor/lazysizes/lazysizes.min.js"></script>
    <script src="~/DoktorKapısı/vendor/isotope/jquery.isotope.min.js"></script>
    <script src="~/DoktorKapısı/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/DoktorKapısı/vendor/magnific-popup/jquery.magnific-popup.min.js"></script>
    <script src="~/DoktorKapısı/vendor/vide/jquery.vide.min.js"></script>
    <script src="~/DoktorKapısı/vendor/vivus/vivus.min.js"></script>

    <!-- Theme Base, Components and Settings -->
    <script src="~/DoktorKapısı/js/theme.js"></script>

    <!-- Circle Flip Slideshow Script -->
    <script src="~/DoktorKapısı/vendor/circle-flip-slideshow/js/jquery.flipshow.min.js"></script>
    <!-- Current Page Views -->
    <script src="~/DoktorKapısı/js/views/view.home.js"></script>

    <!-- Theme Custom -->
    <script src="~/DoktorKapısı/js/custom.js"></script>

    <!-- Theme Initialization Files -->
    <script src="~/DoktorKapısı/js/theme.init.js"></script>

</body>
</html>
